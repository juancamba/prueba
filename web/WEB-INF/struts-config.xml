<struts-config>
    
    <action-mappings>
       
        <action path="/fromhome" forward="/page1.jsp"/>
        
<!--        <action path="/frompage1" forward="/page2.jsp"/>-->
        <action path="/frompage1" 
                type="com.webhomecover.action.Page1Action"
              
